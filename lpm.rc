on early-init
    start ueventd

on init

sysclktz 0

on boot
    # cpu frequency governor
	write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor powersave

    class_start default

## Daemon processes to be run by init.
##
service ueventd /sbin/ueventd
    critical

service battery_charger /charger
    user root
